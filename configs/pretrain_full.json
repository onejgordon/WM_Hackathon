{
  "optimizer": {
    "batch_size": 128,
    "learning_rate": 0.005
  },
  "model": {
    "retina": {
      "f_size": 7,
      "summaries": true
    },
    "cortex": {
      "filters": 100,
      "kernel_size": 4,
      "stride": 2,

      "use_bias": true,
      "use_tied_weights": true,
      "use_lifetime_sparsity": true,

      "encoder_padding": 0,
      "decoder_padding": 0,

      "encoder_nonlinearity": "none",
      "decoder_nonlinearity": "none",

      "sparsity": 10,
      "sparsity_output_factor": 1.0
    }
  }
}
